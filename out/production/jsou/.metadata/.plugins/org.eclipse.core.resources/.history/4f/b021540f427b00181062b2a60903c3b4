package pack1;

public class Test10for {

	public static void main(String[] args) {
		
		int a, hap = 0;	// 누적 기억장소 초기화
		for ( a=1; a<=5; a++ ) {
			System.out.print(a + " ");
//			hap = hap + a;
			hap += a;
		}
		System.out.println("\na:" + a);
		System.out.println("합은 " + hap);
		
		
		
		
		System.out.println("\n프로그램 종료");
	}	// static void main;

}	// class Test10for;
