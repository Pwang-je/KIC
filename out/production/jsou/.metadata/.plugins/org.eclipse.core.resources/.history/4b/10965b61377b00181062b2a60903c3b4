public class Test90629 {

	public static void main(String[] args) {
		
		// Switch 2
		String jik = "과장";
		String msg = "기타";
		
		switch (jik) {
		case "사원":
			msg = "열심히";
			break;
		case "대리":			
		case "과장":
			msg = "우수해";
			break;
		default:
			System.out.println("그외 지급");
		}
		System.out.println(msg);

		
		// 키보드로부터 년과 월을 입력받아 해당년월의 날수를 출력.
		// 4 && 100 :: 400
		
		String str = "평년";
		int year, month, days = 28;
		
		Scanner input = new Scanner(System.in);
		System.out.print("년도");
		year = input.nextInt();
		System.out.print("월");
		month = input.nextInt();
		System.out.println("날수");
		dayz = input.nextInt();
		
		
		
		
		
		
	}

